#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/akeempedro/ontologies/2021/7/safetyontology#Date
cso:Date rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range xsd:dateTime ;
         rdfs:comment "This data property denotes the date of the accident"@en .


###  http://www.semanticweb.org/akeempedro/ontologies/2021/safetyontology#Description
<http://www.semanticweb.org/akeempedro/ontologies/2021/safetyontology#Description> rdf:type owl:DatatypeProperty ;
                                                                                   rdfs:subPropertyOf owl:topDataProperty ;
                                                                                   rdfs:domain owl:Thing ;
                                                                                   rdfs:range xsd:string ;
                                                                                   rdfs:comment "This data property denotes the description of accident case"@en .


###  http://www.semanticweb.org/akeempedro/ontologies/2021/safetyontology#Source
<http://www.semanticweb.org/akeempedro/ontologies/2021/safetyontology#Source> rdf:type owl:DatatypeProperty ;
                                                                              rdfs:subPropertyOf owl:topDataProperty ;
                                                                              rdfs:domain owl:Thing ;
                                                                              rdfs:range xsd:anyURI ;
                                                                              rdfs:comment "This data property denotes the source of accident case"@en .


###  http://www.semanticweb.org/akeempedro/ontologies/2021/safetyontology#Title
<http://www.semanticweb.org/akeempedro/ontologies/2021/safetyontology#Title> rdf:type owl:DatatypeProperty ;
                                                                             rdfs:subPropertyOf owl:topDataProperty ;
                                                                             rdfs:domain owl:Thing ;
                                                                             rdfs:range xsd:string ;
                                                                             rdfs:comment "This data property denotes the title of accident case"@en .


